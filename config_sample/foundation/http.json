{
  "http": {
    "sapi": {
      "web": {
        "listen": [
          {
            "hostname": "localhost",
            "ports": [80, 443]
          }
        ],
        "cors": {
          "$imports": [
            "presets/cors-00"
          ]
        },
        "proxies": [
          {
            "cidr": [
              "127.0.0.1/32",
              "::1/128",
              "10.0.11.0/27",
              "10.0.11.32/27"
            ],
            "xff": false,
            "maxHops": 6,
            "protoFromTrustedEdge": false
          }
        ],
        "wwwSupport": true,
        "enforceTls": true,
        "perRequestConstraints": {
          "$imports": ["presets/security-00"]
        }
      }
    }
  }
}