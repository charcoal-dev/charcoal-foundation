{
  "direct": true,
  "hosts": [
    {
      "hostname": "localhost",
      "ports": [80],
      "tls": false
    },
    {
      "hostname": "foundation.charcoal.dev",
      "ports": [80, 443],
      "tls": {
        "enforced": true,
        "cert": "tls/web/charcoal.dev.crt",
        "key": "tls/web/charcoal.dev.key"
      }
    }
  ],
  "proxies": [
    {
      "cidr": [
        "127.0.0.1/32",
        "::1/128",
        "172.17.0.1/32",
        "10.0.11.0/27",
        "10.0.11.32/27"
      ],
      "xff": true,
      "maxHops": 3,
      "protoFromTrustedEdge": false
    }
  ],
  "cors": {
    "enforced": true,
    "maxAge": 600,
    "origins": [
      "http://localhost:4200",
      "https://foundation.charcoal.dev"
    ]
  },
  "wwwSupport": true,
  "perRequestConstraints": {
    "$imports": ["http/constraints-00"]
  }
}