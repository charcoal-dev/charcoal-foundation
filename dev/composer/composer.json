{
  "require": {
    "php": "^8.3",
    "charcoal-dev/app-kernel": "dev-main",
    "charcoal-dev/http-server": "dev-main#46f442d",
    "charcoal-dev/http-client": "^0.2.11",
    "charcoal-dev/charsets-sanitizer": "^0.2.0",
    "charcoal-dev/mailer": "^0.2.1",
    "ext-pdo": "*",
    "ext-curl": "*",
    "ext-sockets": "*",
    "ext-ctype": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\Shared\\": "../../app/shared/",
      "App\\Domain\\": "../../app/domain/",
      "App\\Sapi\\Engine\\": "../../app/sapi/engine/",
      "App\\Sapi\\Web\\": "../../app/sapi/web/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Charcoal\\Tests\\Foundation\\": "../../tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}